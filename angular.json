{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "resume": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/resume",
      "sourceRoot": "apps/resume/src",
      "prefix": "mi-mono-repo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/resume",
            "index": "apps/resume/src/index.html",
            "main": "apps/resume/src/main.ts",
            "polyfills": "apps/resume/src/polyfills.ts",
            "tsConfig": "apps/resume/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/resume/src/favicon.ico", "apps/resume/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/resume/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["./apps-common/styles"]
            },
            "allowedCommonJsDependencies": ["ngx-store"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/resume/src/environments/environment.ts",
                  "with": "apps/resume/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "resume:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "resume:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "resume:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/resume/tsconfig.app.json",
              "apps/resume/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/resume/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/resume/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/resume"]
        }
      }
    },
    "silo-ngx-doc-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/silo/ngx-doc-app",
      "sourceRoot": "apps/silo/ngx-doc-app/src",
      "prefix": "silo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/silo/ngx-doc-app",
            "index": "apps/silo/ngx-doc-app/src/index.html",
            "main": "apps/silo/ngx-doc-app/src/main.ts",
            "polyfills": "apps/silo/ngx-doc-app/src/polyfills.ts",
            "tsConfig": "apps/silo/ngx-doc-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/silo/ngx-doc-app/src/favicon.ico",
              "apps/silo/ngx-doc-app/src/assets"
            ],
            "styles": [
              "apps/silo/ngx-doc-app/src/styles.scss",
              "node_modules/material-design-icons/iconfont/material-icons.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/silo/ngx-doc-app/src/environments/environment.ts",
                  "with": "apps/silo/ngx-doc-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "silo-ngx-doc-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "silo-ngx-doc-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "silo-ngx-doc-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/silo/ngx-doc-app/tsconfig.app.json",
              "apps/silo/ngx-doc-app/tsconfig.spec.json",
              "apps/silo/ngx-doc-app/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/silo/ngx-doc-app/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/silo/ngx-doc-app/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/silo/ngx-doc-app"]
        }
      }
    },
    "mi-angular-ui": {
      "projectType": "library",
      "root": "libs/mi-angular-ui",
      "sourceRoot": "libs/mi-angular-ui/src",
      "prefix": "mi",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/mi-angular-ui/tsconfig.lib.json",
              "libs/mi-angular-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/mi-angular-ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/mi-angular-ui/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/mi-angular-ui"]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "silo-ngx-lib": {
      "projectType": "library",
      "root": "libs/silo/ngx-lib",
      "sourceRoot": "libs/silo/ngx-lib/src",
      "prefix": "silo",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/silo/ngx-lib/tsconfig.lib.json",
            "project": "libs/silo/ngx-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/silo/ngx-lib/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/silo/ngx-lib/tsconfig.lib.json",
              "libs/silo/ngx-lib/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/silo/ngx-lib/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/silo/ngx-lib/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/silo/ngx-lib"]
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "silo-metadata-lib": {
      "root": "libs/silo/metadata-lib",
      "sourceRoot": "libs/silo/metadata-lib/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/silo/metadata-lib/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/silo/metadata-lib"],
          "options": {
            "jestConfig": "libs/silo/metadata-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "spec": false
    },
    "@schematics/angular:module": {
      "spec": false
    }
  },
  "defaultProject": "resume"
}
